FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      WaterZ;
}

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1E-6;
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
     	type            zeroGradientAbsorbingWall;
        value           uniform 0.0;
    }
}
